{
  "name": "kingmenu-food-planner",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://kamel73-web.github.io/KingMenu",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:legacy": "node --experimental-global-webcrypto node_modules/vite/bin/vite.js build",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:build": "npm run clean && npm run build",
    "deploy": "npm run clean:build && npx gh-pages -d dist --dotfiles --nojekyll --add --remote origin --message \"Deploy: $(date '+%Y-%m-%d %H:%M:%S')\"",
    "deploy:force": "rm -rf dist node_modules/.vite && npm run build && (cd dist && git init && git checkout -b gh-pages && git add -A && git commit -m \"Deploy: $(date)\" && git remote add origin https://github.com/kamel73-web/KingMenu.git && git push origin gh-pages --force)"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.50.2",
    "framer-motion": "^10.16.5",
    "html2canvas": "^1.4.1",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "jspdf": "^2.5.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.5.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.15",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-modal": "^3.16.3",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "gh-pages": "^6.1.1",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.2"
  }
}
